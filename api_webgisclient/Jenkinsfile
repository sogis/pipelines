library identifier: 'jenkins-shared-libs@master', retriever: modernSCM(
    [$class: 'GitSCMSource',
     remote: 'https://github.com/sogis/jenkins-shared-libs.git'])
pipeline {
    agent any 
    options {
        // set a timeout of 10 minutes for this pipeline
        timeout(time: 10, unit: 'MINUTES')
    }
    stages {
        stage ('preamble') {
            steps {
                script {
                    version = stagePreamble.call()
                }
            }
        }
        stage ('Deploy qwc-service') {
            steps {
                build job: 'agi-apps-test-qwc-service', parameters: [ string(name: 'version', value: "${version.version}"), string(name: 'qwcVersion', value: "${version.qwcVersion}"), string(name: 'build', value: "${version.build}") ], wait: true 
                }
            }
        }
    }
